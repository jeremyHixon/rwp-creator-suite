{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "rwp-creator-suite/instagram-banner",
	"version": "1.0.0",
	"title": "Instagram Banner Creator",
	"category": "common",
	"icon": "format-gallery",
	"description": "Upload an image and split it into 3 separate Instagram-optimized images to create a banner effect.",
	"keywords": [
		"instagram",
		"banner",
		"image",
		"split",
		"social",
		"creator"
	],
	"textdomain": "rwp-creator-suite",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./editor.css",
	"style": ["file:./style.css", "file:./style-style.css"],
	"supports": {
		"html": false,
		"align": [ "wide", "full" ],
		"spacing": {
			"padding": true,
			"margin": true
		}
	},
	"attributes": {
		"alignment": {
			"type": "string",
			"default": "none"
		}
	},
	"example": {
		"attributes": {
			"alignment": "center"
		}
	}
}
